@page
@model LogoutModel
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer

<div class="row justify-content-center">
    <div class="col-6">
        <div class="card fb-card c-vitesco-green-04">
            <div class="card-header c-vitesco-green-04">
                <i style="vertical-align:middle; display:inline-block; margin-top:-5px;" class="bi-door-closed c-vitesco-green-04"></i>
                <div class="d-inline-block">
                    <h2 style="color:white">@Localizer["Logout"].Value</h2>
                </div>
            </div>
            <div class="card-block card-padding">
                <br />
                <h3>@Localizer["You have successfully logged out of the application."].Value</h3>
                <br />
            </div>
        </div>
    </div>
</div>
